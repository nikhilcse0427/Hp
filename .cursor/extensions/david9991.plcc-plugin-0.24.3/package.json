			},
			{
				"command": "plcc.canpenconfig",
				"key": "ctrl+l ctrl+c",
				"mac": "cmd+l cmd+c"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"when": "plcc:isIEC61131Proj",
					"command": "plcc.CompileProject",
					"group": "commands"
				},
				{
					"when": "plcc:isIEC61131Proj",
					"command": "plcc.FlashBinary",
					"group": "commands"
				}
			],
			"editor/title": [
				{
					"when": "plcc:isIEC61131Proj",
					"command": "plcc.OpenSimulator",
					"group": "navigation"
				},
				{
					"when": "plcc:isIEC61131Proj",
					"command": "plcc.CompileProject",
					"group": "navigation"
				},
				{
					"when": "plcc:isIEC61131Proj",
