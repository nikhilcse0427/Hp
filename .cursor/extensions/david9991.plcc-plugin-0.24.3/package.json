{
	"name": "plcc-plugin",
	"displayName": "Cloud-based IEC 61131-3 Development Environment",
	"description": "Cloud-based IEC 61131-3 Development Environment.",
	"version": "0.24.3",
	"preview": true,
	"publisher": "david9991",
	"repository": "https://github.com/david9991/plcc-plugin",
	"icon": "images/Logo.png",
	"engines": {
		"vscode": "^1.64.0"
	},
	"categories": [
		"Programming Languages",
		"Linters",
		"Debuggers",
		"Formatters"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./out/main.js",
	"contributes": {
		"snippets": [
			{
				"when": "plcc:isIEC61131Proj",
				"language": "json",
				"path": "./snippets/project-snippets.json"
			}
		],
		"configuration": [
			{
				"title": "PLCC",
				"properties": {
					"plcc.TestingMode": {
						"scope": "resource",
						"type": "boolean",
						"default": false,
						"description": "Testing Mode (Restart required)"
					},
					"plcc.CompilerVersion": {
						"scope": "resource",
						"type": "string",
						"default": "Remote (Latest)",
						"enum": [
							"Remote (Latest)"
						],
						"enumDescriptions": [
							"Latest remote compiler"
						],
						"description": "Compiler Version"
					},
					"plcc.HelperAddress": {
						"scope": "resource",
						"type": "string",
						"default": "ws://127.0.0.1",
						"description": "Helper address (Restart required)"
					},
					"plcc.PlccKey": {
						"scope": "resource",
						"type": "string",
						"default": "",
						"description": "Plcc Key (Restart required)"
					},
					"plcc.PinataApiKey": {
						"scope": "resource",
						"type": "string",
						"default": "",
						"description": "Pinata Api Key (Restart required)"
					},
					"plcc.PinataSecretApiKey": {
						"scope": "resource",
						"type": "string",
						"default": "",
						"description": "Pinata Secret Api Key (Restart required)"
					},
					"plcc.HelperDownloadAddress": {
						"scope": "resource",
						"type": "string",
						"default": "http://le-124.t.pocograph.com",
						"description": "Helper Download Address (Restart required)"
					},
					"plcc.RepositoryAddresses": {
						"scope": "resource",
						"type": "string",
						"default": "http://le-140.t.pocograph.com,http://le-197.t.pocograph.com",
						"description": "Repository Addresses (comma seperated) (Restart required)"
					},
					"plcc.CompilerAddress": {
						"scope": "resource",
						"type": "string",
						"default": "le-132.g.pocograph.com:443",
						"description": "Remote Compiler Address (Restart required)"
					},
					"plcc.Parachain": {
						"scope": "resource",
						"type": "string",
						"default": "wss://plcc-parachain.g.pocograph.com",
						"description": "PLCC Parachain RPC server (Restart required)"
					}
				}
			}
		],
		"customEditors": [
			{
				"viewType": "LadderEditor",
				"displayName": "LadderEditor",
				"selector": [
					{
						"filenamePattern": "*.graph"
					}
				],
				"priority": "default"
			},
			{
				"viewType": "MonitorEditor",
				"displayName": "MonitorEditor",
				"selector": [
					{
						"filenamePattern": "*.monitor"
					}
				],
				"priority": "default"
			},
			{
				"viewType": "MappingEditor",
				"displayName": "MappingEditor",
				"selector": [
					{
						"filenamePattern": "vars.yaml"
					}
				],
				"when": "plcc:isIEC61131Proj",
				"priority": "default"
			},
			{
				"viewType": "EtherCattopology",
				"displayName": "EtherCattopology",
				"selector": [
					{
						"filenamePattern": "*.eni"
					}
				],
				"priority": "default"
			},
			{
				"viewType": "EtherCatConfigEditor",
				"displayName": "EtherCatConfigEditor",
				"selector": [
					{
						"filenamePattern": "*.eccfg"
					}
				],
				"priority": "default"
			}
		],
		"languages": [
			{
				"id": "plcdia",
				"extensions": [
					".graph"
				],
				"aliases": [
					"PLC Diagram"
				]
			},
			{
				"id": "st",
				"extensions": [
					".st"
				],
				"aliases": [
					"Structured Text"
				]
			},
			{
				"id": "datamon",
				"extensions": [
					".monitor"
				],
				"aliases": [
					"PLC Data Visualization"
				]
			}
		],
		"commands": [
			{
				"when": "plcc:isIEC61131Proj",
				"command": "plcc.CompileProject",
				"title": "PLCC: Compile Project",
				"icon": {
					"light": "images/build-light.svg",
					"dark": "images/build-dark.svg"
				}
			},
			{
				"when": "plcc:isIEC61131Proj",
				"command": "plcc.FlashBinary",
				"title": "PLCC: PLC Programming",
				"icon": {
					"light": "images/down-light.svg",
					"dark": "images/down-dark.svg"
				}
			},
			{
				"when": "plcc:isIEC61131Proj",
				"command": "plcc.StartLoging",
				"title": "PLCC: Start Logging",
				"icon": {
					"light": "images/log-start-light.svg",
					"dark": "images/log-start-dark.svg"
				}
			},
			{
				"when": "plcc:isIEC61131Proj",
				"command": "plcc.StopLoging",
				"title": "PLCC: Stop Logging",
				"icon": {
					"light": "images/log-end-light.svg",
					"dark": "images/log-end-dark.svg"
				}
			},
			{
				"when": "plcc:isIEC61131Proj",
				"command": "plcc.Logout",
				"title": "PLCC: Logout"
			},
			{
				"when": "plcc:isIEC61131Proj",
				"command": "plcc.OpenSimulator",
				"title": "PLCC: Open Simulator",
				"icon": {
					"light": "images/play-light.svg",
					"dark": "images/play-dark.svg"
				}
			},
			{
				"command": "plcc.NewAppProject",
				"title": "PLCC: New App Project"
			},
			{
				"command": "plcc.NewLibProject",
				"title": "PLCC: New Lib Project"
			},
			{
				"when": "plcc:isIEC61131Proj",
				"command": "plcc.OpenFBList",
				"title": "PLCC: Open FBList"
			},
			{
				"when": "plcc:isIEC61131Proj",
				"command": "plcc.blockinsertup",
				"title": "PLCC: Block Insert Above"
			},
			{
				"when": "plcc:isIEC61131Proj",
				"command": "plcc.block",
				"title": "PLCC: Insert Below"
			},
			{
				"when": "plcc:isIEC61131Proj",
				"command": "plcc.ld",
				"title": "PLCC: LD"
			},
			{
				"when": "plcc:isIEC61131Proj",
				"command": "plcc.ldn",
				"title": "PLCC: LDN"
			},
			{
				"when": "plcc:isIEC61131Proj",
				"command": "plcc.orbove",
				"title": "PLCC: Orbove"
			},
			{
				"when": "plcc:isIEC61131Proj",
				"command": "plcc.or",
				"title": "PLCC: OR"
			},
			{
				"when": "plcc:isIEC61131Proj",
				"command": "plcc.orn",
				"title": "PLCC: ORN"
			},
			{
				"when": "plcc:isIEC61131Proj",
				"command": "plcc.ldr",
				"title": "PLCC: LDR"
			},
			{
				"when": "plcc:isIEC61131Proj",
				"command": "plcc.coil",
				"title": "PLCC: Coil"
			},
			{
				"when": "plcc:isIEC61131Proj",
				"command": "plcc.coilr",
				"title": "PLCC: Coilr"
			},
			{
				"when": "plcc:isIEC61131Proj",
				"command": "plcc.coils",
				"title": "PLCC: Coils"
			},
			{
				"when": "plcc:isIEC61131Proj",
				"command": "plcc.fb",
				"title": "PLCC: FB"
			},
			{
				"when": "plcc:isIEC61131Proj",
				"command": "plcc.newbranch",
				"title": "PLCC: New Branch"
			},
			{
				"when": "plcc:isIEC61131Proj",
				"command": "plcc.branchup",
				"title": "PLCC: Branch Above"
			},
			{
				"when": "plcc:isIEC61131Proj",
				"command": "plcc.branchdown",
				"title": "PLCC: Branch Below"
			},
			{
				"when": "plcc:isIEC61131Proj",
				"command": "plcc.canpenconfig",
				"title": "PLCC: CanOpen Configuration"
			},
			{
				"when": "plcc:isIEC61131Proj",
				"command": "plcc.esc",
				"title": "PLCC: Esc"
			},
			{
				"when": "plcc:isIEC61131Proj",
				"command": "plcc.etherCatConfig",
				"title": "PLCC: EtherCat Configuration"
			}
		],
		"keybindings": [
			{
				"command": "plcc.blockinsertup",
				"key": "ctrl+l ctrl+t",
				"mac": "cmd+l cmd+t"
			},
			{
				"command": "plcc.block",
				"key": "ctrl+l ctrl+v",
				"mac": "cmd+l cmd+v"
			},
			{
				"command": "plcc.ld",
				"key": "ctrl+l ctrl+/",
				"mac": "cmd+l cmd+l"
			},
			{
